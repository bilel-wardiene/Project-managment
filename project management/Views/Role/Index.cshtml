@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>
<h1>List of roles</h1>
<a asp-action="CreateRole">Create</a>
<table class="table table-striped table-bordered"
    <thead>
        <tr>
            <td>Id</td>
            <td>Name</td>
        </tr>
    </thead>
    <tbody>
        @foreach(var role in Model)
        {
            <tr>
                <td>@role.Id</td>
                <td>@role.Name</td>
                <td> <button type="submit" class="btn btn-danger" asp-action="DeleteConfirmed">Delete</button></td>
                <td><a asp-action="DeleteConfirmed">deleteee</a></td>               
            </tr>
        }
    </tbody>
</table>