@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityUser>
<h1>List of users</h1>
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <td>Id</td>
            <td>Name</td>
        </tr>
    </thead>
    <tbody>
        @foreach(var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.UserName</td>
            </tr>
            

        }
       
    </tbody>
</table>